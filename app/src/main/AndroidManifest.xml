<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="www.dugaolong.com.androidfive">


    <uses-permission android:name="android.permission.CALL_PHONE" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE"/>
    <uses-permission android:name="android.permission.GET_TASKS"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity android:name=".RecycleViewActivity"
            android:theme="@style/AppTheme.Base">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".CardViewActivity"
            android:label="CardViewActivity">
            </activity>
        <activity android:name=".ToolBarActivity"
            android:theme="@style/AppTheme.Base">
        </activity>
        <activity android:name=".RippleEffectActivity"
            android:label="RippleEffectActivity">
        </activity>
        <activity android:name=".TabLayoutActivity"
            android:label="TabLayoutActivity">
        </activity>
        <activity android:name=".MaterialDesignActivity"
            android:label="MaterialDesignActivity">
        </activity>
    </application>

</manifest>